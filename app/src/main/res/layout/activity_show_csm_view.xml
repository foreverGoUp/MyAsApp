<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/llayout_show_csm_view_container"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context="com.kc.activity.ShowCsmViewActivity">

    <com.kc.custom.CsmDialView
        android:id="@+id/csmDialView"
        android:layout_width="200dp"
        android:layout_height="250dp" />

    <com.kc.custom.CsmRadarView
        android:id="@+id/csmRadarView"
        android:layout_marginTop="@dimen/common_blank_nor"
        android:layout_width="250dp"
        android:layout_height="200dp" />
</LinearLayout>
